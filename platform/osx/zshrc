zeesh_plugins=(
    # OS-specific
    osx
    iterm2

    # We live and breath vim here
    vi-mode

    # Fancy command line
    autocomplete
    syntax-highlighting
    history-substring-search

    # Various plugins
    audio
    # autoenv
    git
    # go
    # javascript
    # kubernetes
    python
    virtualenv
    # yarn
    nvm

    # Need to come last
    vcs-info
    theme
    # starship
    # spaceship
)
source ~/.zsh/zeesh.zsh

export PATH=~/.bin:~/.ellipsis/bin:$PATH
alias el=ellipsis

# pnpm
export PNPM_HOME="/Users/z/Library/pnpm"
case ":$PATH:" in
  *":$PNPM_HOME:"*) ;;
  *) export PATH="$PNPM_HOME:$PATH" ;;
esac
# pnpm end