zeesh_plugins=(
    # OS-specific
    osx
    iterm2

    # Various plugins
    audio
    autoenv
    git
    go
    javascript
    # kubernetes
    python
    virtualenv

    # Enable various fancy features
    history-substring-search
    syntax-highlighting

    # We live and breath vim here
    vi-mode

    # Need to come last
    theme
    vcs-info
    autocomplete
)
source ~/.zsh/zeesh.zsh

export PATH=~/.bin:~/.ellipsis/bin:$PATH
alias el=ellipsis
